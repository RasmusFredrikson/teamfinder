<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/matches"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rasmus.teamfinder.MatchesActivity">

    <TextView
        android:id="@+id/playerName1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="28dp"
        android:text="@string/title_home"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toEndOf="@+id/playerImage1"
        app:layout_constraintTop_toTopOf="@+id/playerImage1" />

    <TextView
        android:id="@+id/playerRank1"
        android:layout_width="58dp"
        android:layout_height="19dp"
        android:layout_marginStart="44dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerName1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toEndOf="@+id/playerName1" />

    <TextView
        android:id="@+id/playerPosition1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerRank1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/playerRank1" />

    <ImageView
        android:id="@+id/playerImage1"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <android.support.design.widget.BottomNavigationView
        android:id="@+id/navigation"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/navigation" />

    <ImageView
        android:id="@+id/playerImage3"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="@+id/playerImage2"
        app:layout_constraintTop_toBottomOf="@+id/playerImage2"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/playerImage5"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="@+id/playerImage4"
        app:layout_constraintTop_toBottomOf="@+id/playerImage4"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/playerImage4"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="@+id/playerImage3"
        app:layout_constraintTop_toBottomOf="@+id/playerImage3"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/playerImage2"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="@+id/playerImage1"
        app:layout_constraintTop_toBottomOf="@+id/playerImage1"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/playerName2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/playerName1"
        app:layout_constraintTop_toTopOf="@+id/playerImage2" />

    <TextView
        android:id="@+id/playerRank3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerName3"
        app:layout_constraintStart_toStartOf="@+id/playerRank2" />

    <TextView
        android:id="@+id/playerRank4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerName4"
        app:layout_constraintStart_toStartOf="@+id/playerRank3" />

    <TextView
        android:id="@+id/playerRank5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerName5"
        app:layout_constraintStart_toStartOf="@+id/playerRank4" />

    <TextView
        android:id="@+id/playerPosition2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerRank2"
        app:layout_constraintStart_toStartOf="@+id/playerPosition1" />

    <TextView
        android:id="@+id/playerPosition3"
        android:layout_width="58dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerRank3"
        app:layout_constraintStart_toStartOf="@+id/playerPosition2" />

    <TextView
        android:id="@+id/playerPosition4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerRank4"
        app:layout_constraintStart_toStartOf="@+id/playerPosition3" />

    <TextView
        android:id="@+id/playerPosition5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerRank5"
        app:layout_constraintStart_toStartOf="@+id/playerPosition4" />

    <TextView
        android:id="@+id/playerName4"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/playerName3"
        app:layout_constraintTop_toTopOf="@+id/playerImage4" />

    <TextView
        android:id="@+id/playerName5"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/playerName4"
        app:layout_constraintTop_toTopOf="@+id/playerImage5" />

    <TextView
        android:id="@+id/playerRank2"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerName2"
        app:layout_constraintStart_toStartOf="@+id/playerRank1" />

    <TextView
        android:id="@+id/playerName3"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/playerName2"
        app:layout_constraintTop_toTopOf="@+id/playerImage3" />

    <ImageView
        android:id="@+id/playerImage6"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:src="@mipmap/ic_launcher_round"
        app:layout_constraintStart_toStartOf="@+id/playerImage5"
        app:layout_constraintTop_toBottomOf="@+id/playerImage5" />

    <TextView
        android:id="@+id/playerName6"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:textStyle="bold"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/playerName5"
        app:layout_constraintTop_toTopOf="@+id/playerImage6" />

    <TextView
        android:id="@+id/playerRank6"
        android:layout_width="57dp"
        android:layout_height="wrap_content"
        android:textColor="@color/colorPrimaryDark"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerName6"
        app:layout_constraintStart_toStartOf="@+id/playerRank5" />

    <TextView
        android:id="@+id/playerPosition6"
        android:layout_width="57dp"
        android:textColor="@color/colorPrimaryDark"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/playerRank6"
        app:layout_constraintStart_toStartOf="@+id/playerPosition5" />

</android.support.constraint.ConstraintLayout>
